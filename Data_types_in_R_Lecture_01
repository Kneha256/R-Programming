#logical_data_types
a<-TRUE
a<-T
class(a)
typeof(a)
c<-TRUE
c
typeof(c)

#numeric_data_types
class(a)
a<-3
class(a)
a<-3.4
class(a)
c<-3
class(c)
typeof(c)
ball<-5
ball
typeof(ball)

#integer_data_types
a<-3L
typeof(a)
a
class(a)
b<-9L
b
typeof(b)

#Complex_Data_Type
a<-3+4i
a
class(a)

#Raw_Data_Types

#single_inverted_and_double_inverted_commas_uses
a<-'A'
class(a)
a<-"hello"
class(a)

#concandate
class(a)
a<-10
b<-20
cat(a,b)

